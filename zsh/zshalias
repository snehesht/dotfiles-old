##################Git###################

# Add
alias ga="git add $@"
alias gaa="git add ."

# Commit 
alias gc="git commit -m $@"

# Log
alias gl="git log --graph --decorate --pretty=oneline --abbrev-commit --all --date=local"

# Push & Pull
alias gph="git push $@"
alias gpl="git pull"

# Branch
alias gb="git branch $@"

# Checkout 
alias gk="git checkout $@"

# Diff
alias gd="git diff $@"
